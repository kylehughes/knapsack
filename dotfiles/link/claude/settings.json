  "hooks": {
    "Notification": [
      {
        "matcher": "idle_prompt",
        "hooks": [
          {
            "type": "command",
            "command": "terminal-notifier -title 'Claude Code' -message 'Waiting for input' -sound default"
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "~/.claude/statusline.sh"
  },
  "enabledPlugins": {
    "apple-platform-build-tools@apple-platform-build-tools": true,
    "migrating-to-swift-concurrency-skill@the-unofficial-swift-concurrency-migration-skill": true,
    "programming-swift-skill@the-unofficial-swift-programming-language-skill": true,
    "swift-lsp@claude-plugins-official": true
  },
  "extraKnownMarketplaces": {
    "apple-platform-build-tools-claude-code-plugin": {
      "source": {
        "source": "github",
        "repo": "kylehughes/apple-platform-build-tools-claude-code-plugin"
      }
    },
    "the-unofficial-swift-concurrency-migration-skill": {
      "source": {
        "source": "github",
        "repo": "kylehughes/the-unofficial-swift-concurrency-migration-skill"
      }
    },
    "the-unofficial-swift-programming-language-skill": {
      "source": {
        "source": "github",
        "repo": "kylehughes/the-unofficial-swift-programming-language-skill"
      }
    }
  },
  "alwaysThinkingEnabled": true,
  "skipDangerousModePermissionPrompt": true
}
